contract C {
  // Packed in slot 0
  uint8 a;
  uint8 b;
  uint16 c;

  // New slot 1
  uint256 d;

  // Packed in slot 2
  bytes4 e;
  bytes4 f;

  function rw() public {
    a = a;
    b = b;
    c = c;
    d = d;
    e = e;
    f = f;
  }
}

