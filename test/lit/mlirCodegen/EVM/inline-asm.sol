// RUN: solc --mlir-action=print-std-mlir --mlir-target=evm --mmlir --mlir-print-debuginfo %s | FileCheck %s

function f() {
  assembly {
    mstore(0, 0)
  }
}

contract C {
  uint8 a;    // slot 0, packable
  uint256 b;  // slot 1, non-packable

  function extRef() public pure returns (uint) {
    uint x = 1;
    assembly {
      x := add(x, 1)
    }
    return x;
  }

  function slotOffset() public pure returns (uint, uint, uint, uint) {
    uint sa; uint oa; uint sb; uint ob;
    assembly {
      sa := a.slot
      oa := a.offset
      sb := b.slot
      ob := b.offset
    }
    return (sa, oa, sb, ob);
  }
}

