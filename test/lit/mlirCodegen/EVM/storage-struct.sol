// RUN: solc --mlir-action=print-std-mlir --mlir-target=evm --mmlir --mlir-print-debuginfo %s | FileCheck %s

contract C {
  // Packed struct
  struct Packed {
    uint8 a;
    uint8 b;
    uint16 c;
  }
  Packed packed;

  // Non-packed struct
  struct NonPacked {
    uint256 x;
    uint256 y;
  }
  NonPacked nonPacked;

  // Nested struct
  struct Inner {
    uint8 x;
    uint8 y;
  }
  struct Outer {
    Inner inner;
    uint8 z;
  }
  Outer nested;

  // Mixed struct (value + reference types)
  struct Mixed {
    uint8 x;
    uint256[] arr;
    uint8 y;
  }
  Mixed mixed;

  function rw() public {
    packed.a = packed.b;
    nonPacked.x = nonPacked.y;
    nested.inner.x = nested.z;
    mixed.x = mixed.y;
    mixed.arr.push(1);
  }
}

