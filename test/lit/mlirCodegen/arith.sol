// RUN: solc --mlir-action=print-init --mmlir --mlir-print-debuginfo %s | FileCheck %s

function f_ui(uint256 a) {}
function f_si(int256 a) {}

function unchk_ui(uint256 a, uint256 b) {
  unchecked {
    a += b;
    a -= b;
    a *= b;

    f_ui(a + b);
    f_ui(a - b);
    f_ui(a * b);
  }
}

function unchk_si(int256 a, int256 b) {
  unchecked {
    a += b;
    a -= b;
    a *= b;

    f_si(a + b);
    f_si(a - b);
    f_si(a * b);
  }
}

function chk_ui(uint256 a, uint256 b) {
  a += b;
  a -= b;

  f_ui(a + b);
  f_ui(a - b);
}

function chk_si(int256 a, int256 b) {
  a += b;
  a -= b;

  f_si(a + b);
  f_si(a - b);
}

