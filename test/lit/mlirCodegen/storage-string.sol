contract Test {
  bytes str;
  bytes str2;

  function p() public {
    str.push(0xdd);
  }

  function p2() public {
    str.push() = 0x65;
  }

  function pop() public {
    str.pop();
  }

  function p3() public returns (uint) {
    return str.length;
  }

  function m(bytes memory a) public {
    str = a;
  }

  function ss() public {
    str2 = str;
  }

  function cs(bytes calldata a) public {
    str = a;
  }

  function m2() public returns (bytes memory) {
    return str;
  }

  function m3() public returns (bytes1) {
    return str[10];
  }

  function m4() public {
    str[10] = 0x41;
  }

  function m3dyn(uint idx) public returns (bytes1) {
    return str[idx];
  }

  function m4dyn(uint idx) public {
    str[idx] = 0x41;
  }
}
