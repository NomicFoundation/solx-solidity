  function p() public returns (uint) {
    str.push(0xdd);
    return str.length;
  }

  function p2() public returns (uint) {
    str.pop();
    return str.length;
  }

  function m(bytes memory a) public returns (bytes memory) {
    str = a;
    return str;
  }
}

